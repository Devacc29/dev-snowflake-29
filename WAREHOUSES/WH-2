USE ROLE SECURITYADMIN;

CREATE OR REPLACE WAREHOUSE DEF_WS_WH_1
WITH
WAREHOUSE_TYPE = STANDARD
WAREHOUSE_SIZE = XSMALL
MAX_CLUSTER_COUNT = 3
MIN_CLUSTER_COUNT = 1
INITIALLY_SUSPENDED = TRUE
COMMENT = 'it will not create';

SHOW WAREHOUSES;

DESCRIBE WAREHOUSE DEF_WS_WH_1;

ALTER WAREHOUSE DEF_WS_WH_1 
SET 
WAREHOUSE_SIZE = SMALL
AUTO_SUSPEND = 300;

